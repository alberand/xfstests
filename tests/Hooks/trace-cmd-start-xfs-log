# trace-cmd start hook for tracing XFS log events
#
# This is a memory hog - it reserves 250MB of RAM per CPU for the ring buffer.
hook_execute()
{
	echo Running hook $_hook
	trace-cmd start -b 250000 -e xfs_log\* -e xlog\*
}
